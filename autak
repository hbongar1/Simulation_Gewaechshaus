import pypsa   

network = pypsa.Network()                                                                                
network.set_snapshots(range(24))    

network.add('Bus', name = 'Strom')                                                                        
network.add('Bus', name = 'Waerme')  

network.add('Generator', name = 'Windkraftanlage', bus = 'Strom', p_nom = 30)     
network.add('Link', name = 'Waermepumpe', bus0 = 'Strom', bus1 = 'Waerme', p_nom = 7, efficiency = 3)     
network.add('Load', name = 'Gewaechshaus_Waerme', bus = 'Waerme', p_set = 10)                             
network.add('Generator', name = 'Netzstrom', bus = 'Strom', p_nom = 100000000)                            
network.add('Load', name = 'Gewaechshaus_Strom', bus = 'Strom', p_set = 5)                               

return network


# Frage:
# Hat die Außentemperatur und die Solareinstrahlung nicht direkte Auswirkungen auf die benötigte Wärmelast 
# des Gewächshauses und nicht auf die Wärmepumpe bzw. Wärmeknoten?
